
numero_secreto = 6

tentativa = int(input("qual é o numero secreto?"))

print(type(tentativa))

if numero_secreto == tentativa: 
    print("Parabéns você acertou!")


elif numero_secreto < tentativa:
    print( "Errou! Menor que o número secreto")

elif numero_secreto > tentativa:
    print("Errou! Maior que o numero secreto")
else:
    print("Vaor invalido")
